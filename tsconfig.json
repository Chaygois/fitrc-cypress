{
    "compilerOptions": {
      "target": "ES2022",
      "lib": ["ES2023"],
      "module": "ESNext",
      "moduleResolution": "node",
      "strict": true,
      "noEmit": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "resolveJsonModule": true
    },
    "include": [
      "cypress/js/*.ts",
      "cypress.config.ts"
    ]
  }
  